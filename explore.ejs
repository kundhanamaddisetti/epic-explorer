        @keyframes animate {
            0%{
            opacity: 0;
            transform: translateY(100%) scale(0.5);
            }
            5%, 20%{
            opacity: 0.4;
            transform: translateY(100%) scale(0.7);
            }
            25%, 40%{
            opacity: 1;
            pointer-events: auto;
            transform: translateY(0%) scale(1);
            }
            45%, 60%{
            opacity: 0.4;
            transform: translateY(-100%) scale(0.7);
            }
            65%, 100%{
            opacity: 0;
            transform: translateY(-100%) scale(0.5);
            }
